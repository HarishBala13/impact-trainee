{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 119",
    "not firefox <= 119"
  ],
  "hints": {
    "no-inline-styles": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scale"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "marquee": "off"
      }
    ],
    "css-prefix-order": "off",
    "detect-css-reflows/composite": "off"
  }
}